@using BOL;
@{
    ViewData["Title"] = "Details page";
}

<div>
    @{
    Product product = (Product)ViewData["prod"];

    <p>ID:@product.Id</p>
    <p>Name:@product.Name</p>
    <p>Category:@product.Category</p>
    <p>UnitPrice:@product.UnitPrice</p>
    <p>ExpDate:@product.ExpDate</p>

}
</div>